{
  "name": "llx-launchpad",
  "main": "src/main.html",
  "id":"corellian-launcher",
  "window": {
    "show": true,
    "title": "LliureX Launchpad Title",
    "width": 640,
    "height": 480,
    "show_in_taskbar": false,
    "frame":false,
    "visible-on-all-workspaces":true,
    "toolbar":false,
    "transparent":true
    },
  "webkit": {
    "plugin": true
  },
  "chromium-args": "--ignore-certificate-errors --disable-setuid-sandbox --no-sandbox --allow-no-sandbox-job --mixed-context",
   "devDependencies": {
    "jquery": "^2.1",
    "jquery-ui": "^1.10.4",
    "xdg": "^0.1",
    "xdg-env": "^0.0.3",
    "dbus-native": "^0.2.0",
    "deep-diff": "^0.3.3"
    }

}
